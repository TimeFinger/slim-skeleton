{
    "name": "TimeFinger/slim-skeleton",
    "description": "My slim sketeton",
    "require": {
        "slim/slim": "^3.10",
        "catfan/Medoo": "^1.5",
        "vlucas/phpdotenv": "^2.5",
        "slim/php-view": "^2.2.0",
        "symfony/cache": "^4.1",
        "predis/predis": "^1.1",
        "wp-cli/php-cli-tools": "^0.11.10",
        "monolog/monolog": "^1.24"
    },
    "autoload": {
        "psr-4": {
            "Application\\Cores\\":"src/Cores/",
            "Application\\Modules\\":"src/Modules/",
            "Application\\Models\\":"src/Models/",
            "Application\\Tasks\\":"src/Tasks/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^6.2"
    }
}
